    "scripts": {
        "prod": "node src/server.js",
        "dev": "tsup-node src/server.ts --watch --on-success \"cls && node --env-file=.env dist/server.js | pino-pretty --colorize\"",
        "prebuild": "shx rm -rf dist || rmdir /s /q dist",
        "build": "tsc -p tsconfig.json",
        "preview": "node --env-file=.env dist/server.js",
        "db:gen": "npx prisma generate",
        "db:studio": "npx prisma studio",
        "db:migrate": "npx  prisma migrate dev",
        "db:push": "npx run prisma db push",
        "db:seed": "ts-node prisma/seed.ts"
    },
    "dependencies": {
